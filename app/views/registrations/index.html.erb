<p id="notice"><%= notice %></p>

<h1>Registrations</h1>

<table>
  <thead>
    <tr>
      <th>Regno</th>
      <th>Subno</th>
      <th>Regdate</th>
      <th>Remdate</th>
      <th>Remcode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registrations.each do |registration| %>
      <tr>
        <td><%= registration.regno %></td>
        <td><%= registration.subno %></td>
        <td><%= registration.regdate %></td>
        <td><%= registration.remdate %></td>
        <td><%= registration.remcode %></td>
        <td><%= link_to 'Show', registration %></td>
        <td><%= link_to 'Edit', edit_registration_path(registration) %></td>
        <td><%= link_to 'Destroy', registration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registration', new_registration_path %>
